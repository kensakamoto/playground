    "# S = \"aaaaaabbbbaaaabbbbbbbaaaa\"\n",
    "# V=[]\n",
    "\n",
    "def run_length(S):\n",
    "    V=[]\n",
    "\n",
    "    for s in S:\n",
    "        if len(V)==0 or V[-1][0]!=s:\n",
    "            V.append([s,1])\n",
    "        else:\n",
    "            V[-1][1] += 1\n",
    "\n",
    "    s=\"\"\n",
    "    for v in V:\n",
    "        s += \"%s%d\" % (v[0],v[1])\n",
    "    return s"
   ]
